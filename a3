  mutate(
    c1_bud_Aluminum_Extraction=i1_bud_Aluminum*b1_Aluminum_Extraction/1000,
    c1_bud_Aluminum_Manufacturing=i1_bud_Aluminum*b1_Aluminum_Manufacturing/1000,
    c1_bud_Aluminum_Operational=i1_bud_Aluminum*b1_Aluminum_Operational/1000,
    c1_bud_Aluminum_Transportation=i1_bud_Aluminum*b1_Aluminum_Transportation/1000,
    c1_bud_Aluminum_End=i1_bud_Aluminum*b1_Aluminum_End/1000,
    
    c1_bud_Steel_Extraction=i1_bud_Steel*b1_Steel_Extraction/1000,
    c1_bud_Steel_Manufacturing=i1_bud_Steel*b1_Steel_Manufacturing/1000,
    c1_bud_Steel_Operational=i1_bud_Steel*b1_Steel_Operational/1000,
    c1_bud_Steel_Transportation=i1_bud_Steel*b1_Steel_Transportation/1000,
    c1_bud_Steel_End=i1_bud_Steel*b1_Steel_End/1000,
    
    c1_bud_Copper_Extraction=i1_bud_Copper*b1_Copper_Extraction/1000,
    c1_bud_Copper_Manufacturing=i1_bud_Copper*b1_Copper_Manufacturing/1000,
    c1_bud_Copper_Operational=i1_bud_Copper*b1_Copper_Operational/1000,
    c1_bud_Copper_Transportation=i1_bud_Copper*b1_Copper_Transportation/1000,
    c1_bud_Copper_End=i1_bud_Copper*b1_Copper_End/1000,
    
    c1_bud_Cement_Extraction=i1_bud_Cement*b1_Cement_Extraction/1000,
    c1_bud_Cement_Manufacturing=i1_bud_Cement*b1_Cement_Manufacturing/1000,
    c1_bud_Cement_Operational=i1_bud_Cement*b1_Cement_Operational/1000,
    c1_bud_Cement_Transportation=i1_bud_Cement*b1_Cement_Transportation/1000,
    c1_bud_Cement_End=i1_bud_Cement*b1_Cement_End/1000,
    
    c1_bud_Gravel_Extraction=i1_bud_Gravel*b1_Gravel_Extraction/1000,
    c1_bud_Gravel_Manufacturing=i1_bud_Gravel*b1_Gravel_Manufacturing/1000,
    c1_bud_Gravel_Operational=i1_bud_Gravel*b1_Gravel_Operational/1000,
    c1_bud_Gravel_Transportation=i1_bud_Gravel*b1_Gravel_Transportation/1000,
    c1_bud_Gravel_End=i1_bud_Gravel*b1_Gravel_End/1000,
    
