  pull() -> r1_Aluminum_Operational
mfd %>% 
  dplyr::filter(link=="r1_Aluminum") %>% 
  dplyr::select(Transportation) %>% 
  pull() -> r1_Aluminum_Transportation
mfd %>% 
  dplyr::filter(link=="r1_Aluminum") %>% 
  dplyr::select(End) %>% 
  pull() -> r1_Aluminum_End


