mfd %>% 
  dplyr::filter(link=="r1_Aluminum") %>% 
  dplyr::select(Extraction) %>% 
  pull() -> r1_Aluminum_Extraction
mfd %>% 
  dplyr::filter(link=="r1_Aluminum") %>% 
  dplyr::select(Manufacturing) %>% 
  pull() -> r1_Aluminum_Manufacturing
