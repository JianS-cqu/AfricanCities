    c5_bud_Glass_Operational=i5_bud_Glass*b5_Glass_Operational/1000,
    c5_bud_Glass_Transportation=i5_bud_Glass*b5_Glass_Transportation/1000,
    c5_bud_Glass_End=i5_bud_Glass*b5_Glass_End/1000,
    
    c5_bud_Wood_Extraction=i5_bud_Wood*b5_Wood_Extraction/1000,
    c5_bud_Wood_Manufacturing=i5_bud_Wood*b5_Wood_Manufacturing/1000,
    c5_bud_Wood_Operational=i5_bud_Wood*b5_Wood_Operational/1000,
    c5_bud_Wood_Transportation=i5_bud_Wood*b5_Wood_Transportation/1000,
    c5_bud_Wood_End=i5_bud_Wood*b5_Wood_End/1000,
    
    c5_bud_Brick_Extraction=i5_bud_Brick*b5_Brick_Extraction/1000,
    c5_bud_Brick_Manufacturing=i5_bud_Brick*b5_Brick_Manufacturing/1000,
    c5_bud_Brick_Operational=i5_bud_Brick*b5_Brick_Operational/1000,
    c5_bud_Brick_Transportation=i5_bud_Brick*b5_Brick_Transportation/1000,
    c5_bud_Brick_End=i5_bud_Brick*b5_Brick_End/1000,
    
    c5_bud_Asphalt_Extraction=i5_bud_Asphalt*b5_Asphalt_Extraction/1000,
    c5_bud_Asphalt_Manufacturing=i5_bud_Asphalt*b5_Asphalt_Manufacturing/1000,
    c5_bud_Asphalt_Operational=i5_bud_Asphalt*b5_Asphalt_Operational/1000,
    c5_bud_Asphalt_Transportation=i5_bud_Asphalt*b5_Asphalt_Transportation/1000,
    c5_bud_Asphalt_End=i5_bud_Asphalt*b5_Asphalt_End/1000,
    
    c5_bud_Plastic_Extraction=i5_bud_Plastic*b5_Plastic_Extraction/1000,
    c5_bud_Plastic_Manufacturing=i5_bud_Plastic*b5_Plastic_Manufacturing/1000,
    c5_bud_Plastic_Operational=i5_bud_Plastic*b5_Plastic_Operational/1000,
    c5_bud_Plastic_Transportation=i5_bud_Plastic*b5_Plastic_Transportation/1000,
    c5_bud_Plastic_End=i5_bud_Plastic*b5_Plastic_End/1000,
    
    c5_bud_Rubber_Extraction=i5_bud_Rubber*b5_Rubber_Extraction/1000,
    c5_bud_Rubber_Manufacturing=i5_bud_Rubber*b5_Rubber_Manufacturing/1000,
    c5_bud_Rubber_Operational=i5_bud_Rubber*b5_Rubber_Operational/1000,
    c5_bud_Rubber_Transportation=i5_bud_Rubber*b5_Rubber_Transportation/1000,
    c5_bud_Rubber_End=i5_bud_Rubber*b5_Rubber_End/1000) -> df1

readxl::read_xlsx("AFCC/excel/carbonIntensity2.xlsx") %>% 
  dplyr::filter(Sectors=="roads") %>% 
  dplyr::select(-Sectors) %>% 
  unite("link",Product, Materials) %>% 
  spread(Process, Carbon) -> mfd




