readxl::read_xlsx("AFCC/excel/carbonIntensity2.xlsx") %>% 
  dplyr::filter(Sectors=="energy") %>% 
  dplyr::select(-Sectors) %>% 
  unite("link",Product, Materials) %>% 
  spread(Process, Carbon) -> mfd





##r1  
mfd %>% 
  dplyr::filter(link=="e1_Aluminum") %>% 
  dplyr::select(Extraction) %>% 
  pull() -> e1_Aluminum_Extraction
mfd %>% 
  dplyr::filter(link=="e1_Aluminum") %>% 
  dplyr::select(Manufacturing) %>% 
  pull() -> e1_Aluminum_Manufacturing
mfd %>% 
  dplyr::filter(link=="e1_Aluminum") %>% 
  dplyr::select(Operational) %>% 
  pull() -> e1_Aluminum_Operational
mfd %>% 
  dplyr::filter(link=="e1_Aluminum") %>% 
  dplyr::select(Transportation) %>% 
  pull() -> e1_Aluminum_Transportation
mfd %>% 
  dplyr::filter(link=="e1_Aluminum") %>% 
  dplyr::select(End) %>% 
  pull() -> e1_Aluminum_End


