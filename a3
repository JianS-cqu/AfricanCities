


##3. from material to carbon emissions
library(tidyverse)

read_csv("./inflow_ALL_mat.csv") -> df


readxl::read_xlsx("./carbonIntensity2.xlsx") %>% 
  dplyr::filter(Sectors=="building") %>% 
  dplyr::select(-Sectors) %>% 
  unite("link",Product, Materials) %>% 
  spread(Process, Carbon) -> mfd


##b1  
mfd %>% 
  dplyr::filter(link=="b1_Aluminum") %>% 
  dplyr::select(Extraction) %>% 
  pull() -> b1_Aluminum_Extraction
mfd %>% 
  dplyr::filter(link=="b1_Aluminum") %>% 
  dplyr::select(Manufacturing) %>% 
  pull() -> b1_Aluminum_Manufacturing
mfd %>% 
  dplyr::filter(link=="b1_Aluminum") %>% 
  dplyr::select(Operational) %>% 
  pull() -> b1_Aluminum_Operational
mfd %>% 
  dplyr::filter(link=="b1_Aluminum") %>% 
  dplyr::select(Transportation) %>% 
  pull() -> b1_Aluminum_Transportation
mfd %>% 
  dplyr::filter(link=="b1_Aluminum") %>% 
  dplyr::select(End) %>% 
  pull() -> b1_Aluminum_End

